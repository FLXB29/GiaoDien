{
  "manifest_version": 3,
  "name": "NT Load Chatgpt",
  "version": "1.0",
  "description": "Nhập dữ liệu vào trình duyệt",
  "action": {
    "default_popup": "popup.html",
	"default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
	}
  },
  "permissions": [
    "cookies",
    "activeTab",
    "storage",
    "system.cpu",
    "system.memory",
    "system.display"
	],
  "host_permissions": [
    "<all_urls>"
  ]
}